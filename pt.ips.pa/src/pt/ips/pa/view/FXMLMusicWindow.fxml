<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ips.pa.controller.FXMLMusicWindowController">
   <children>
      <BorderPane prefHeight="150.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <Slider fx:id="slider" blockIncrement="0.1" majorTickUnit="0.25" max="1.0" showTickLabels="true" showTickMarks="true" value="0.25" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <Button fx:id="buttonPlayPause" mnemonicParsing="false" onAction="#buttonAction" text="Pause" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <Label fx:id="titulo" text="Musica" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </top>
         <padding>
            <Insets bottom="15.0" left="20.0" right="20.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
